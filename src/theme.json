{
    "name": "Catppuccin",
    "author": "ozwaldorf, soradotwav",
    "manifest_version": 3,
    "target": "Other",
    "description": "Catppuccin Steam Deck",
    "inject": {
        "shared.css": ["SP", "MainMenu", "QuickAccess"]
    },
    "patches": {
        "Flavor": {
            "type": "slider",
            "values": {
                "Mocha": {
                    "flavors/mocha.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Macchiato": {
                    "flavors/macchiato.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Frappe": {
                    "flavors/frappe.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Latte": {
                    "flavors/latte.css": ["SP", "MainMenu", "QuickAccess"]
                }
            }
        },
        "Accent": {
            "type": "dropdown",
            "values": {
                "Rosewater": {
                    "accents/rosewater.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Flamingo": {
                    "accents/flamingo.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Pink": {
                    "accents/pink.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Mauve": {
                    "accents/mauve.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Red": {
                    "accents/red.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Maroon": {
                    "accents/maroon.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Peach": {
                    "accents/peach.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Yellow": {
                    "accents/yellow.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Green": {
                    "accents/green.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Teal": {
                    "accents/teal.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Sky": {
                    "accents/sky.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Sapphire": {
                    "accents/sapphire.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Blue": {
                    "accents/blue.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Lavender": {
                    "accents/lavender.css": ["SP", "MainMenu", "QuickAccess"]
                }
            }
        },
        "Enable Keyboard": {
            "type": "checkbox",
            "values": {
                "Yes": {
                    "keyboard.css": ["SP", "SP Keyboard"]
                },
                "No": {}
            }
        } 
    }
}