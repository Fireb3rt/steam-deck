{
    "name": "Gruvbox Material",
    "author": "Firebert, ozwaldorf, soradotwav",
    "manifest_version": 3,
    "target": "Other",
    "description": "Gruvbox Material Steam Deck",
    "inject": {
        "shared.css": ["SP", "MainMenu", "QuickAccess"]
    },
    "patches": {
        "Palette": {
            "type": "slider",
            "values": {
                "Mocha": {
                    "palette/mocha.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Dark Medium": {
                    "palette/dark-medium.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Frappe": {
                    "palette/frappe.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Latte": {
                    "palette/latte.css": ["SP", "MainMenu", "QuickAccess"]
                }
            }
        },
        "Accent": {
            "type": "dropdown",
            "values": {
                "Red": {
                    "accents/red.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Orange": {
                    "accents/orange.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Yellow": {
                    "accents/yellow.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Green": {
                    "accents/green.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Aqua": {
                    "accents/aqua.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Blue": {
                    "accents/blue.css": ["SP", "MainMenu", "QuickAccess"]
                },
                "Purple": {
                    "accents/purple.css": ["SP", "MainMenu", "QuickAccess"]
                }
            }
        }
    }
}